/**index.wxss**/

page {
  background-color: #f5f5f5;
}

.swiperimg {
  width: 100%;
  height: 100%;
}

.video {
  width: 100%;
  background-color: #fff;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}

video {
  width: 96%;
  height: 460rpx;
  margin: 0 2%;
}

.classification {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
}

/*分类样式  */

.in1 {
  background-color: #fff;
  width: 100%;
  height: auto;
  padding-top: 25rpx;
  display: flex;
  box-sizing: border-box;
  flex-wrap: wrap;
  justify-content: flex-start;
  border-bottom: 1rpx solid #ddd;
}

.in1_1 {
  justify-content: center;
  flex-wrap: wrap;
  box-sizing: border-box;
  display: flex;
  width: 25%;
  margin-bottom: 10rpx;
}

.in1_1image image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 180rpx;
}

.in1_1text {
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  color: #353535;
}

.info {
  width: 100%;
  margin-top: 10rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.infoheader {
  width: 100%;
  font-size: 32rpx;
  background-color: #64FFBF;
  color: #404646;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
}

.infobady {
  width: 96%;
  justify-content: center;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.infoimg {
  width: 100%;
  height: 300rpx;
  border-radius: 10rpx;
}

.infotext {
  width: 100%;
  display: flex;
  color: #353535;
  box-sizing: border-box;
  padding: 10rpx 10rpx;
  background-color: #fff;
}

.logo {
  width: 100%;
  display: flex;
  justify-content: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  flex-wrap: wrap;
}

.logo text {
  width: 100%;
  display: flex;
  justify-content: center;
  text-align: center;
  flex-wrap: wrap;
  font-size: 28rpx;
  color: gray;
  margin-top: 10rpx;
}

.logoimg {
  width: 100%;
  display: flex;
  justify-content: center;
}

/* 最新资讯 */

.infos {
  width: 100%;
  background: #fff;
  margin-bottom: 8px;
  padding: 2%;
  box-sizing: border-box;
}

.info-contents {
  width: 62%;
  float: right;
  font-size: 32rpx;
}

.info-images {
  width: 38%;
  text-align: left;
  float: left;
}

.info-images image {
  width: 250rpx;
  height: 150rpx;
}

.i_txt {
  width: 100%;
  margin-bottom: 15rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.i_cat {
  color: #b2b2b2;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 15rpx;
}

.free{
  color: #00B26A;
}
.pay{
  color: #f50;
}

.connect image {
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  bottom: 50rpx;
  right: 60rpx;
  border-radius: 50rpx;
  z-index: 9999999999999999999999999;
}

.connect contact-button {
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  bottom: 28rpx;
  right: 170rpx;
  border-radius: 50rpx;
  z-index: 99999999999999;
}

#background-image {
  width: 70rpx;
  height: 70rpx;
  position: fixed;
  bottom: 55rpx;
  right: 194rpx;
  border-radius: 50rpx;
  z-index: 9999999999999999999999999;
}

.list{
  width: 100%;
  display: flex;
  align-items: center;
  padding: 25rpx;
  box-sizing: border-box;
  background-color: #FFF;
  border-bottom: 2rpx solid #f5f5f5;
}
.list .left-image{
  display:flex;
  align-items:center;
}
.list .left-image image{
  width: 50rpx;
  height: 50rpx;
}
.list .text{
  flex: 1;
  padding-left: 20rpx;
}
.list .right-image{
  display: flex;
  align-items: center;
} 
.list .right-image text{
  padding-right: 20rpx;
  color: #f50;
}
.list .right-image image{
  width: 30rpx;
  height: 30rpx;
}